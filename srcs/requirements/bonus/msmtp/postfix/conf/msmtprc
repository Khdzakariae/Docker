defaults
auth           on
tls            on
tls_starttls   on
tls_trust_file /etc/ssl/certs/ca-certificates.crt
logfile        /var/log/msmtp.log

account gmail
host           GMAIL_HOST_HERE
port           GMAIL_PORT_HERE
from           GMAIL_USER_HERE
user           GMAIL_USER_HERE
password       GMAIL_PASS_HERE

account default : gmail
